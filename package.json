{
  "name": "ng2-jest-webpack",
  "version": "0.0.1",
  "description": "Seed(angular2 && jest && webpack2)",
  "main": "index.ts",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "lint": "tslint ./app/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scalamorphing/ng2-jest-webpack.git"
  },
  "keywords": [
    "angular2",
    "webpack2",
    "jest",
    "seed"
  ],
  "author": "Maxim Chepel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scalamorphing/ng2-jest-webpack/issues"
  },
  "homepage": "https://github.com/scalamorphing/ng2-jest-webpack#readme",
  "devDependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@types/core-js": "^0.9.35",
    "@types/reflect-metadata": "0.0.5",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.3",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.0.1",
    "ts-jest": "^19.0.0",
    "ts-loader": "^2.0.1",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.2.1",
    "typings": "^2.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "zone.js": "^0.7.7"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "^.*.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
